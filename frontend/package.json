    "@vue/tsconfig": "^0.8.1",
    "electron": "^40.3.0",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "jiti": "^2.6.1",
